# cd
alias d='cd ~/Desktop'
alias repo='cd ~/Desktop/repositories'
alias doc='cd ~/Desktop/documents'
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'

# ls
alias la='ls -aF'
alias ll='ls -alF'

# Git
alias g='git'
alias ga='git add'
alias gai='git add -i'
alias gd='git diff'
alias gds='git diff --staged'
alias gs='git status'
alias gp='git push'
alias gb='git branch'
alias gco='git checkout'
alias gf='git fetch'
alias gu='git fetch --all --prune'
alias gcm='git commit'
alias gcl='git clone'
alias gl='git log'
alias gg='git log --graph --oneline --all'
alias gln='git log --name-only --oneline'
alias gm='git merge'
alias grb='git rebase'
alias grv='git revert'
alias gcp='git cherry-pick'

